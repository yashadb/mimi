{
	"settings": {
		"dateFormat": "YYYY-MM-DD",
		"paginationLimit": 10,
		"language": "en"
	},
	"seo": {
		"title": "Welcome to My Website",
		"description": "Discover amazing content and learn more about us.",
		"keywords": [
			"homepage",
			"welcome",
			"blog",
			"content"
		],
		"extra": []
	},
	"postTypes": [
		{
			"name": "page",
			"directory": "resources/pages"
		}
	],
	"inserts": {
		"head": [
			{
				"tag": "description-meta",
				"script": "<meta name=\"description\" content=\"Custom description for the page\">"
			},
			{
				"tag": "stylesheet",
				"script": "<link rel=\"stylesheet\" href=\"https://example.com/styles.css\">"
			},
			{
				"tag": "analytics-script",
				"script": "<script src=\"https://example.com/head-script.js\" async></script>"
			}
		],
		"endOfBody": [
			{
				"tag": "footer-script",
				"script": "<script src=\"https://example.com/footer-script.js\"></script>"
			},
			{
				"tag": "custom-logger",
				"script": "<script>console.log('Custom script at the end of body');</script>"
			}
		]
	}
}
